{
    "name": "wdio-cucumber-example",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "wdio run wdio.conf.js",        
        "wdio": "wdio run wdio.conf.js",
        "awards": "wdio wdio.conf.js --spec ./test/features/awards-page.feature",
        "navbar": "wdio wdio.conf.js --spec ./test/features/navbar.feature",
        "searchbar": "wdio wdio.conf.js --spec ./test/features/search-bar-imdb.feature",
        "toprated": "wdio wdio.conf.js --spec ./test/features/top-rated-movies.feature",
        "steps": "node ./node_modules/.bin/cucumber-js test/features/*.feature --format-options '{\"snippetInterface\":\"async-await\"}'"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@babel/preset-env": "^7.16.4",
        "@babel/register": "^7.16.0",
        "@wdio/cli": "^7.16.10",
        "@wdio/cucumber-framework": "^7.16.6",
        "@wdio/dot-reporter": "^7.16.6",
        "@wdio/local-runner": "^7.16.10",
        "@wdio/spec-reporter": "^7.16.9",
        "chromedriver": "^96.0.0",
        "wdio-chromedriver-service": "^7.2.2"
    },
    "dependencies": {
        "@cucumber/cucumber": "^8.0.0-rc.1",
        "cucumber": "^7.0.0-rc.0"
    }
}
